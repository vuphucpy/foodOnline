# Container Commands
container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python manage.py migrate"
    leader_only: true
  02_createsuperuser:
    command: "source /var/app/venv/*/bin/activate && echo from accounts.models import User; User.objects.create_superuser('vu', 'phuc', 'admin@gmail.com', 'vuphucadmin', 'Gettingstarted#47') | python manage.py shell"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: foodOnline_main.settings