# Container Commands
container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python manage.py migrate"
    leader_only: true
  02_createsuperuser:
    command: "source /var/app/venv/*/bin/activate && python manage.py shell -c 'from accounts.models import User; User.objects.create_superuser(\"vu\", \"phuc\", \"vuphucadmin\", \"admin@gmail.com\", \"Gettingstarted#47\")'"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: foodOnline_main.settings